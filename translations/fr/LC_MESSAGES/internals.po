msgstr "argon2-cffi_ 16.1.0+"

msgid "asgiref_ 3.2+ (required)"
msgstr "asgiref_ 3.2+ (obligatoire)"

msgid "bcrypt_"
msgstr "bcrypt_"

msgid "docutils_"
msgstr "docutils_"

msgid "geoip2_"
msgstr "geoip2_"

msgid "jinja2_ 2.7+"
msgstr "jinja2_ 2.7+"

msgid "numpy_"
msgstr "numpy_"

msgid "Pillow_"
msgstr "Pillow_"

msgid "PyYAML_"
msgstr "PyYAML_"

msgid "pytz_ (required)"
msgstr "pytz_ (obligatoire)"

msgid "pywatchman_"
msgstr "pywatchman_"

msgid "setuptools_"
msgstr "setuptools_"

msgid "memcached_, plus a :ref:`supported Python binding <memcached>`"
msgstr "memcached_, plus une :ref:`liaison Python prise en charge <memcached>`"

msgid "gettext_ (:ref:`gettext_on_windows`)"
msgstr "gettext_ (:ref:`gettext_on_windows`)"

